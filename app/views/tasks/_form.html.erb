<%= bootstrap_form_for(task) do |f| %>
  
  <%= f.text_field :title %><br/>

  <%= f.text_field :content %><br/>

  <%=f.datetime_select :start_time, :locale => 'en'%><br/>

  <%=f.datetime_select :end_time, :locale => 'en'%><br/>
  
  <%= f.select :status, t(:status).values %><br/>

  <%= f.select :priority, t(:priority).values %><br/>

  <%= f.text_field :tag %><br/>

  <%= f.text_field :user_id %><br/>

  <%= f.submit t("button.submit")%>
<% end %>